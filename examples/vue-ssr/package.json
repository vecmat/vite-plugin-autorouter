{
    "private": true,
    "scripts": {
        "dev": "nodemon --watch ../../dist/*.js -x 'esno server'",
        "build": "pnpm build:client && pnpm build:server",
        "build:client": "vite build --ssrManifest --outDir dist/client",
        "build:server": "vite build --ssr src/entry-server.ts --outDir dist/server",
        "generate": "cross-env DEBUG=vite-plugin-autorouter:* vite build --ssrManifest --outDir dist/static && yarn build:server && esno prerender",
        "serve": "cross-env NODE_ENV=production esno server"
    },
    "dependencies": {
        "vue": "^3.2.20",
        "vue-router": "^4.0.12"
    },
    "devDependencies": {
        "@types/compression": "^1.7.2",
        "@types/express": "^4.17.13",
        "@types/node": "^16.11.6",
        "@vitejs/plugin-vue": "^1.9.4",
        "@vue/compiler-sfc": "^3.2.20",
        "@vue/server-renderer": "^3.2.20",
        "compression": "^1.7.4",
        "cross-env": "^7.0.3",
        "esno": "^0.10.1",
        "express": "^4.17.1",
        "fast-glob": "^3.2.7",
        "nodemon": "^2.0.14",
        "serve-static": "^1.14.1",
        "typescript": "^4.4.4",
        "vite": "^2.6.13",
        "vite-plugin-autorouter": "workspace:*",
        "vite-plugin-restart": "^0.0.2"
    }
}
